{
  "name": "tjk-stablemate",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "dev": "next dev",
    "prebuild": "npx prisma generate && npx prisma migrate deploy",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prepare": "husky install",
    "postinstall": "prisma generate && (npx playwright install chromium --with-deps || echo 'Playwright browser installation skipped')",
    "db:seed": "ts-node --compiler-options {\\\"module\\\":\\\"CommonJS\\\"} prisma/seed.ts",
    "db:migrate": "prisma migrate dev",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "db:truncate": "./scripts/truncate-db.sh",
    "db:create-demo": "ts-node --compiler-options {\\\"module\\\":\\\"CommonJS\\\"} scripts/create-demo-data.ts",
    "db:delete-demo": "ts-node --compiler-options {\\\"module\\\":\\\"CommonJS\\\"} scripts/delete-demo-data.ts",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "cronjob:nightly": "tsx scripts/nightly-data-update.ts"
  },
  "dependencies": {
    "@next-auth/prisma-adapter": "^1.0.7",
    "@playwright/test": "^1.40.0",
    "@prisma/client": "6.8.2",
    "@radix-ui/react-alert-dialog": "1.1.4",
    "@radix-ui/react-checkbox": "^1.1.2",
    "@radix-ui/react-dialog": "1.1.14",
    "@radix-ui/react-label": "^2.1.1",
    "@radix-ui/react-popover": "1.1.14",
    "@radix-ui/react-progress": "^1.1.1",
    "@radix-ui/react-radio-group": "^1.2.2",
    "@radix-ui/react-select": "^2.1.4",
    "@radix-ui/react-slot": "^1.1.1",
    "@radix-ui/react-switch": "^1.1.2",
    "@radix-ui/react-tabs": "^1.1.2",
    "@stripe/stripe-js": "^5.5.0",
    "@types/react-datepicker": "^6.2.0",
    "bcryptjs": "^2.4.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "1.1.1",
    "csv-parser": "3.2.0",
    "date-fns": "4.1.0",
    "jose": "^6.1.1",
    "js-cookie": "^3.0.5",
    "jsonwebtoken": "9.0.2",
    "lucide-react": "^0.553.0",
    "next": "14.1.0",
    "next-auth": "^4.24.11",
    "playwright": "^1.40.0",
    "prisma": "6.8.2",
    "qrcode.react": "^4.2.0",
    "react": "^18.2.0",
    "react-datepicker": "^8.9.0",
    "react-dom": "^18.2.0",
    "recharts": "^2.15.4",
    "resend": "4.0.1",
    "sharp": "0.33.5",
    "sonner": "^1.7.1",
    "stripe": "^17.5.0",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/js-cookie": "^3.0.6",
    "@types/jsonwebtoken": "9.0.9",
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "autoprefixer": "^10.0.0",
    "eslint": "^8.0.0",
    "eslint-config-next": "14.1.0",
    "husky": "8.0.3",
    "postcss": "^8.0.0",
    "tailwindcss": "^3.3.0",
    "ts-node": "10.9.2",
    "tsx": "^4.7.0",
    "typescript": "^5.0.0"
  }
}
